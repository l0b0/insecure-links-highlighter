{
  "manifest_version": 2,
  "name": "Insecure Links Highlighter",
  "version": "1.0",
  "description": "Highlight insecure links",
  "author": "Victor Engmark",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "highlight.js"
      ]
    }
  ],
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  }
}
